{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "linebreak-style": 0,
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [
          "Link"
        ],
        "specialLink": [
          "to"
        ]
      }
    ]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "parser": "babel-eslint"
}